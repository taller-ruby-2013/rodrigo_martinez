<h1> Nuevo Post</h1>
<!--FORMULARIO EN RUBY-->
<%= form_for :post,url:posts_path  do |f| %>
	<p>
	<%= f.label :title %><br>
	<%= f.text_field :title %>
	</p>
	<p>
	<%= f.label :text %><br>
	<%= f.text_area :text %>
	</p>
	<p>
	<%= f.submit %>
	</p>
<%end %>